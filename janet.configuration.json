{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        ["[", "]"],
        ["(", ")"],
        ["{", "}"]
    ],
    "autoClosingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["{", "}"],
        { "open": "\"", "close": "\"", "notIn": ["string"] },
        { "open": "`", "close": "`", "notIn": ["string", "comment"] }
    ],
    "autoCloseBefore": ";:.,=<([{}])>` \n\t\\w",
    "surroundingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["{", "}"],
        ["\"", "\""],
        ["`", "`"]
    ],
    "wordPattern": "[A-Za-z0-9\\!\\$\\%\\&\\*\\+\\-\\.\\/\\:\\<\\?\\=\\>\\@\\^\\_\\|\\x80-\\xFF]+"
}
